      "@id": "kern:ierlang",
      "name": "IErlang",
      "url": "https://github.com/robbielynch/ierlang"
    },
    "kern:iforth": {
      "@id": "kern:iforth",
      "name": "IForth",
      "url": "https://github.com/jdfreder/iforth"
