    "kern:ifsharp": {
      "@id": "kern:ifsharp",
      "name": "IFSharp",
      "url": "http://nbviewer.ipython.org/github/BayardRock/IfSharp/blob/master/Feature%20Notebook.ipynb"
    },
    "kern:igo": {
      "@id": "kern:igo",
      "name": "IGo",
      "url": "https://github.com/takluyver/igo"
    },
    "kern:ihaskell": {
      "@id": "kern:ihaskell",
      "name": "IHaskell",
      "url": "http://nbviewer.ipython.org/github/gibiansky/IHaskell/blob/master/notebooks/IHaskell.ipynb"
