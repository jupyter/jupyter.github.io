    "kern:ijavascript": {
      "@id": "kern:ijavascript",
      "name": "IJavascript",
      "url": "https://www.npmjs.com/package/ijavascript"
    },
    "kern:ijulia": {
      "@id": "kern:ijulia",
      "actions": {
        "act:github": "gh:JuliaLang/IJulia.jl",
        "act:try": "try:IJulia"
      },
      "description": "IJulia is a Julia-language backend combined with the IPython interactive environment (soon to be called Jupyter). This combination allows you to interact with the Julia language using Jupyter/IPython's powerful graphical notebook, which combines code, formatted text, math, and multimedia in a single document.",
      "features": [
        "feat:autocomplete",
        "feat:docs",
        "feat:langext",
        "feat:widgets"
      ],
      "languages": {
        "lang:julia": {
          "versions": ["0.3.8"]
        }
      },
      "logo": "https://cdn.rawgit.com/JuliaLang/IJulia.jl/master/deps/logo-64x64.png",
      "name": "IJulia",
      "url": "http://nbviewer.ipython.org/url/jdj.mit.edu/~stevenj/IJulia%20Preview.ipynb",
      "version": "0.4"
    },
    "kern:imathics": {
      "@id": "kern:imathics",
      "name": "IMathics",
      "url": "http://nbviewer.ipython.org/gist/sn6uv/8381447"
    },
    "kern:imatlab": {
      "@id": "kern:imatlab",
      "name": "IMatlab",
      "url": "https://github.com/calysto/matlab_kernel"
    },
    "kern:iocaml": {
      "@id": "kern:iocaml",
