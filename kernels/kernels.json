    },
    "kern:ioctave": {
      "@id": "kern:ioctave",
      "name": "IOctave",
      "url": "https://github.com/calysto/octave_kernel"
    },
    "kern:iperl": {
      "@id": "kern:iperl",
      "name": "IPerl",
      "url": "https://metacpan.org/release/Devel-IPerl"
    },
    "kern:iperl6": {
      "@id": "kern:iperl6",
      "name": "IPerl6",
      "url": "https://github.com/timo/iperl6kernel"
    },
    "kern:ipython": {
      "@id": "kern:ipython",
      "actions": {
        "act:docker": "docker:ipython/notebook",
        "act:github": "gh:ipython/ipython",
        "act:try": "try:IPython"
      },
      "description": "The original Jupyter Kernel, IPython provides Python, a general purpose programming language.",
      "features": [
        "feat:autocomplete",
        "feat:docs",
        "feat:langext",
        "feat:widgets"
      ],
      "languages": {
        "lang:python": {
          "versions": ["2.7", "3.3", "3.4"]
        }
      },
      "logo": "python/logo-64x64.png",
      "name": "IPython",
      "version": "3.0",
      "url": "http://ipython.org/"
    },
    "kern:irkernel": {
      "@id": "kern:irkernel",
      "actions": {
        "act:docker": "docker:adrianliaw/jupyter-irkernel",
        "act:try": "try:ir"
      },
      "description": "This is still experimental. Your code should be safe, since IPython handles saving and loading notebooks in another process, but you'll lose all your variables if it crashes.",
      "features": [
        "feat:autocomplete",
        "feat:langext"
      ],
      "languages": {
        "lang:r": {
          "versions": ["3.2.0", "3.1.3"]
        }
      },
      "logo": "https://cdn.rawgit.com/IRkernel/IRkernel/master/inst/kernelspec/logo-64x64.png",
      "name": "IRKernel",
      "version": "0.4",
      "url": "https://github.com/takluyver/IRkernel"
    },
