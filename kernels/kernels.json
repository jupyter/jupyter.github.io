{
  "@context": {
    "act": "jupy:action/",
    "actions": {
      "@container": "@index",
      "@id": "act#action",
      "@type": "@id"
    },
    "description": "rdfs:description",
    "docker": "https://registry.hub.docker.com/u/",
    "fa": "http://fortawesome.github.io/Font-Awesome/icons/",
    "feat": "kern:kernels/features/",
    "features": {
      "@container": "@index",
      "@id": "kern:#feature",
      "@type": "@id"
    },
    "gh": "https://github.com/",
    "icon": "fa:#icon",
    "jupy": "http://jupyter.org/",
    "kern": "jupy:kernels/",
    "kernels": {
      "@container": "@index",
      "@id": "kern:#kernel",
      "@type": "@id"
    },
    "lang": "jupy:Language/",
    "languages": {
      "@container": "@index",
      "@id": "lang:#language",
      "@type": "@id"
    },
    "name": "rdfs:label",
    "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "try": "http://try.jupyter.org/new_kernel=",
    "twbs": "http://getbootstrap.com/css/",
    "twbs-context": "twbs:#helper-classes-colors/"
  },
  "actions": {
    "act:docker": {
      "@id": "act:docker",
      "icon": "fa:cube",
      "name": "Download Containerized Kernel"
    },
    "act:github": {
      "@id": "act:github",
      "icon": "fa:github",
      "name": "Contribute on GitHub"
    },
    "act:try": {
      "@id": "act:try",
      "icon": "fa:rocket",
      "name": "Try on try.jupyter.org"
    }
  },
  "features": {
    "feat:autocomplete": {
      "@id": "feat:autocomplete",
      "icon": "fa:flash",
      "name": "Completion",
      "description": "Shows possible completions of currently-loaded symbols"
    },
    "feat:docs": {
      "@id": "feat:docs",
      "icon": "fa:bookmark",
      "name": "Live Doc",
      "description": "Shows documentation of a currently loaded symbol"
    },
    "feat:langext": {
      "@id": "feat:langext",
      "icon": "fa:level-up",
      "name": "Metas",
      "description": "Extends the implemented language with text symbols"
    },
    "feat:widgets": {
      "@id": "feat:widgets",
      "icon": "fa:sliders",
      "name": "Widgets",
      "description": "Offers reactive UI elements in the frontend linked to backend data"
    }
  },
  "kernels": {
    "kern:bash": {
      "@id": "kern:bash",
      "name": "Bash",
      "url": "https://github.com/takluyver/bash_kernel"
    },
    "kern:brython-kernel": {
      "@id": "kern:brython-kernel",
      "name": "Brython Kernel",
      "url": "https://github.com/kikocorreoso/brythonmagic"
    },
    "kern:calico-project": {
      "@id": "kern:calico-project",
      "name": "Calico Project",
      "url": "http://nbviewer.ipython.org/urls/bitbucket.org/ipre/calico/raw/master/notebooks/Calico%20Overview.ipynb"
    },
    "kern:calysto-processing": {
      "@id": "kern:calysto-processing",
      "name": "Calysto Processing",
      "url": "https://github.com/Calysto/calysto_processing"
    },
    "kern:calysto-scheme": {
      "@id": "kern:calysto-scheme",
      "name": "Calysto Scheme",
      "url": "https://github.com/Calysto/calysto_scheme",
      "version": "0.8.2",
      "languages" : {
        "lang:scheme": {
          "versions": ["2", "3"]
        }
      },
      "logo": "https://cdn.rawgit.com/Calysto/calysto_scheme/917d855dcae3ebc1f6f2c2024a48c78570421f30/calysto_scheme/images/logo-64x64.png",
      "features": [
        "feat:autocomplete",
        "feat:docs",
        "feat:langext"
      ],
      
      "actions": {
        "act:github": "gh:Calysto/calysto_scheme"
      },
      "description": "Calysto Scheme is a real Scheme programming language with full support for continuations, including call/cc. It can also use all Python libraries. It has some extensions that make it useful (stepper-debugger, choose/fail, stack traces), or make it integrated with Python."
    },
    "kern:clojure-kernel": {
      "@id": "kern:clojure-kernel",
      "name": "Clojure Kernel",
      "url": "https://github.com/roryk/ipython-clojure"
    },
    "kern:hy-kernel": {
      "@id": "kern:hy-kernel",
      "name": "Hy Kernel",
      "url": "https://github.com/bollwyvl/hy_kernel/"
    },
    "kern:ialdor": {
